{
  "name": "RNBoilerplate",
  "version": "0.1.0",
  "scripts": {
    "start": "./node_modules/react-native-cli/index.js start",
    "bundle-ios": "./node_modules/react-native-cli/index.js bundle --platform ios --dev false --entry-file index.ios.js --bundle-output ios/RNBoilerplate/index.ios.bundle --assets-dest ios/RNBoilerplate/Assets.xcassets --minify",
    "bundle-android": "./node_modules/react-native-cli/index.js bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ --minify",
    "bundle-all": "npm run bundle-ios && npm run bundle-android",
    "ios-open": "open ios/*.xcodeproj",
    "android": "./node_modules/react-native-cli/index.js run-android",
    "lint": "eslint src test",
    "test": "mocha --recursive --compilers js:babel-core/register -r babel-polyfill -r test/setup",
    "test-watch": "npm run test -- --watch",
    "cover": "babel-node node_modules/.bin/isparta cover _mocha -- --recursive -r test/setup",
    "postinstall": "./postinstall.sh",
    "newproject": "./newproject.sh"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^3.10.1",
    "react-native": "~0.16.0",
    "react-redux": "^3.1.2",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-polyfill": "^6.3.14",
    "babel-preset-rn": "~0.16.0",
    "chai": "^3.4.1",
    "enzyme": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.11.3",
    "fbjs-scripts": "^0.5.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "mockery": "^1.4.0",
    "navigator": "^1.0.1",
    "node-fetch": "^1.3.3",
    "proxyquire": "^1.7.3",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-native-cli": "^0.1.7",
    "react-native-web": "0.0.10",
    "redux-logger": "^2.3.1",
    "remote-redux-devtools": "0.0.5",
    "remote-redux-devtools-on-debugger": "0.0.3",
    "replace": "^0.3.0",
    "rnpm": "^1.3.4",
    "sinon": "^1.17.2"
  },
  "devEngines": {
    "node": ">= 4.1"
  }
}
